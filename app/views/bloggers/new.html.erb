<h1>Bloggers#new</h1>
<p>Find me in app/views/bloggers/new.html.erb</p>

<%= form_for @blogger do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br/>
    <%= f.label :age %>
    <%= f.number_field :age %>
    <br/>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
    <br/>
    <%= f.submit "Create Blogger" %>
<% end %>
